evaluations = data;\r\n            })\r\n        },\r\n        handleEdit(){\r\n            this.editVisible = true;\r\n        },\r\n        saveEdit(){\r\n            let _this = this;\r\n            console.log(localStorage.getItem('ms_id'));\r\n            axios.get('/api/student/student/'+ localStorage.getItem('ms_id')).then(function(res) {\r\n                let data = res.data.data;\r\n                _this.form.coachId = data.coachId;\r\n                _this.form.studentId = data.id;\r\n                _this.form.evaTime = new Date();\r\n                _this.saveEva(data.coachId);\r\n            })\r\n        },\r\n        saveEva(coachId){\r\n            let _this = this;\r\n            axios.post('/api/student/evaluation',this.form).then(function(res) {\r\n                console.log(\"提交\")\r\n                this.flush()\r\n            });\r\n            _this.editVisible = false;\r\n            _this.$message.success(\"评论成功\");\r\n        },\r\n        flush(){\r\n            console.log(\"刷新\")\r\n            this.$router.push('/studentEvaluation')\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n    .el-collapse-item__header {\r\n        padding: 0 10px;\r\n    }\r\n</style>\r\n"]}]}