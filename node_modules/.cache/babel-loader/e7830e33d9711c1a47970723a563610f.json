form-item>\r\n                        <el-form-item label=\"电子邮箱\">\r\n                            <el-input\r\n                                    v-model=\"coach.email\">\r\n                            </el-input>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"地址\">\r\n                            <el-input\r\n                                    v-model=\"coach.address\">\r\n                            </el-input>\r\n                        </el-form-item>\r\n                        <el-form-item>\r\n                            <el-button type=\"primary\" @click=\"update\">修改完成</el-button>\r\n                        </el-form-item>\r\n                    </el-form>\r\n                </div>\r\n            </div>\r\n            <div class=\"container\" style=\"float: left\">\r\n                <div>\r\n                    <div class=\"margin-box\">\r\n                        <span>车辆信息</span>\r\n                    </div>\r\n                    <el-table\r\n                            :data=\"cars\"\r\n                            style=\"width: 100%; text-align: center\">\r\n                        <el-table-column\r\n                                prop=\"id\"\r\n                                label=\"车牌编号\"\r\n                                width=\"120px\">\r\n                        </el-table-column>\r\n                        <el-table-column\r\n                                prop=\"name\"\r\n                                label=\"车辆名称\"\r\n                                width=\"120px\">\r\n                        </el-table-column>\r\n                        <el-table-column\r\n                                prop=\"number\"\r\n                                label=\"车牌号\"\r\n                                width=\"120px\">\r\n                        </el-table-column>\r\n                        <el-table-column\r\n                                prop=\"number\"\r\n                                label=\"操作\"\r\n                                width=\"120px\">\r\n                            <template slot-scope=\"scope\">\r\n                                <el-button\r\n                                        type=\"text\"\r\n                                        icon=\"el-icon-delete\"\r\n                                        class=\"red\"\r\n                                        @click=\"handleDelete(scope.row.id, scope.$index, scope.row)\"\r\n                                >删除</el-button>\r\n                            </template>\r\n                        </el-table-column>\r\n                    </el-table>\r\n                    <div style=\"margin-top: 15px\">\r\n                        <span style=\"margin-right: -5px\">\r\n                              <el-button @click=\"handleAdd\"  type=\"success\">\r\n                                  <i class=\"el-icon-plus\"></i>\r\n                                  新增车辆\r\n                              </el-button>\r\n                    </span>\r\n                    </div>\r\n                    <!-- 编辑弹出框 -->\r\n                    <el-dialog title=\"编辑\" :visible.sync=\"editVisible\" width=\"30%\">\r\n                        <el-form ref=\"form\" :model=\"form\" label-width=\"70px\">\r\n                            <el-form-item label=\"车辆名称\">\r\n                                <el-input v-model=\"car.name\"></el-input>\r\n                            </el-form-item>\r\n                            <el-form-item label=\"车牌号\">\r\n                                <el-input v-model=\"car.number\"></el-input>\r\n                            </el-form-item>\r\n                        </el-form>\r\n                <el-button @click=\"editVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"addCar\">确 定</el-button>\r\n                </el-dialog>\r\n                </div>\r\n            </div>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import axios from 