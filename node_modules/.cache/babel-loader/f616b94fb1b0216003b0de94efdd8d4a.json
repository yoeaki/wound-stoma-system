div class=\"drag-box-item\" style=\"float: left; margin-top: 60px; width: 400px !important;max-width:400px !important;\">\r\n                    <div class=\"item-title\">基本信息</div>\r\n                    <div class=\"container\">\r\n                        <div>\r\n                            <el-row class=\"coach-box\">\r\n                                <el-col class=\"text-label\" :span=\"6\">名称：</el-col>\r\n                                <el-col  class=\"coach-data\":span=\"18\">{{coach.username}}</el-col>\r\n                            </el-row>\r\n                        </div>\r\n                        <div>\r\n                            <el-row class=\"coach-box\">\r\n                                <el-col class=\"text-label\" :span=\"6\">性别：</el-col>\r\n                                <el-col  class=\"coach-data\":span=\"18\">{{coach.sex}}</el-col>\r\n                            </el-row>\r\n                        </div>\r\n                        <div>\r\n                            <el-row class=\"coach-box\">\r\n                                <el-col class=\"text-label\" :span=\"6\">联系电话：</el-col>\r\n                                <el-col  class=\"coach-data\":span=\"18\">{{coach.phone}}</el-col>\r\n                            </el-row>\r\n                        </div>\r\n                        <div>\r\n                            <el-row class=\"coach-box\">\r\n                                <el-col class=\"text-label\" :span=\"6\">参考报价：</el-col>\r\n                                <el-col  class=\"coach-data\":span=\"18\">{{coach.price}}</el-col>\r\n                            </el-row>\r\n                        </div>\r\n                        <div>\r\n                            <el-row class=\"coach-box\">\r\n                                <el-col class=\"text-label\" :span=\"6\">电子邮件：</el-col>\r\n                                <el-col  class=\"coach-data\":span=\"18\">{{coach.email}}</el-col>\r\n                            </el-row>\r\n                        </div>\r\n                        <div>\r\n                            <el-row class=\"coach-box\">\r\n                                <el-col class=\"text-label\" :span=\"6\">地址：</el-col>\r\n                                <el-col  class=\"coach-data\":span=\"18\">{{coach.address}}</el-col>\r\n                            </el-row>\r\n                        </div>\r\n\r\n                    </div>\r\n                </div>\r\n                <div class=\"drag-box-item\" style=\"float: left; margin-top: 60px; width:45% !important;max-width: unset !important;\" >\r\n                    <div class=\"item-title\">教练车辆信息</div>\r\n                    <div class=\"container\">\r\n                        <div>\r\n                            <el-table\r\n                                    :data=\"cars\"\r\n                                    style=\"width: 100%\">\r\n                                <el-table-column\r\n                                        prop=\"id\"\r\n                                        label=\"车牌编号\"\r\n                                        width=\"180\">\r\n                                </el-table-column>\r\n                                <el-table-column\r\n                                        prop=\"name\"\r\n                                        label=\"车辆名称\"\r\n                                        width=\"180\">\r\n                                </el-table-column>\r\n                                <el-table-column\r\n                                        prop=\"number\"\r\n                                        label=\"车牌号\">\r\n                                </el-table-column>\r\n                            </el-table>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n</template>\r\n\r\n<script>\r\n    import axios from 'axios'\r\n    export default {\r\n        name: 'draglist',\r\n        data() {\r\n            return {\r\n                coach:{\r\n                    username: '',\r\n                    sex: '',\r\n                    phone: '',\r\n                    email:'',\r\n                    address: '',\r\n                    price:''\r\n                },\r\n                cars:[]\r\n            }\r\n        },\r\n        created(){\r\n            this.query();\r\n        },\r\n        methods: {\r\n            removeHandle(event){\r\n                console.log(event);\r\n                this.$message.success(`从 ${event.from.id} 移动到 ${event.to.id} `);\r\n            },\r\n            query(){\r\n                let _this = this;\r\n                axios.get('api/student/student/pro/' + localStorage.getItem(\"ms_id\")).then(function(res) {\r\n                    let data = res.data;\r\n                    console.log(data)\r\n                    console.log(data.status == 0)\r\n                    if (data.status == 0) {\r\n                        console.log(data.data.coachId);\r\n                        _this.queryById(data.data.coachId);\r\n                        _this.queryCar(data.data.coachId);\r\n                    }\r\n                })\r\n            },\r\n            queryById(coachId){\r\n                let _this = this;\r\n                axios.get('api/coach/coach/pro/' + coachId).then(function(res) {\r\n                    let data = res.data.data;\r\n                    console.log(data)\r\n                    _this.coach.username = data.username;\r\n                    _this.coach.sex = data.sex;\r\n                    _this.coach.phone = data.phone;\r\n                    _this.coach.email = data.email;\r\n                    _this.coach.address = data.address;\r\n                    _this.coach.price = data.price;\r\n                })\r\n            },\r\n            queryCar(coachId){\r\n                let _this = this;\r\n                axios.get('api/coach/car/coach/' + coachId).then(function(res) {\r\n                    let data = res.data.data;\r\n                    _this.cars = data;\r\n                    console.log(_this.car)\r\n                })\r\n            }\r\n        }\r\n    }\r\n\r\n</script>\r\n\r\n<style scoped>\r\n    .text-label {\r\n        text-align: right;\r\n    }\r\n    .coach-data {\r\n        text-align: left;\r\n    }\r\n    .coach-box {\r\n        padding: 20px 0px;\r\n    }\r\n    .drag-box{\r\n        display: flex;\r\n        user-select: none;\r\n    }\r\n    .drag-box-item {\r\n        flex: 1;\r\n        max-width: 330px;\r\n        min-width: 300px;\r\n        background-color: #eff1f5;\r\n        margin-right: 16px;\r\n        border-radius: 6px;\r\n        border: 1px #e1e4e8 solid;\r\n    }\r\n    .item-title{\r\n        padding: 8px 8px 8px 12px;\r\n        font-size: 14px;\r\n        line-height: 1.5;\r\n        color: #24292e;\r\n        font-weight: 600;\r\n    }\r\n\r\n</style>\r\n"],"sourceRoot":"src/components/user/student"}]}